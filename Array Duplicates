lass Solution {
    public List<Integer> findDuplicates(int[] arr) {
        // code here
        List<Integer> res = new ArrayList<>();
        HashSet<Integer> set = new HashSet<>();
        
        for(int i=0; i<arr.length; i++){
            if(set.contains(arr[i])) res.add(arr[i]);
            else set.add(arr[i]);
        }
        return res;
    }
}
